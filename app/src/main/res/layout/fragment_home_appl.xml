<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.ContentFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.HomeApplFragment">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycle_home_appliance"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:layout_width="wrap_content"
        android:id="@+id/fab_chat"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/extra_margin"
        android:visibility="gone"
        app:backgroundTint="@color/colorPrimary"
        app:rippleColor="@color/colorWhite"
        app:srcCompat="@drawable/ic_message" />

</androidx.appcompat.widget.ContentFrameLayout>

